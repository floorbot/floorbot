{
    "name": "floorbot",
    "version": "1.0.0",
    "description": "The collection and consolidation of the different floorbot features",
    "main": "./dist/app.js",
    "type": "module",
    "scripts": {
        "depcheck": "depcheck",
        "build": "tsc",
        "watch": "tsc-watch --onSuccess \"node --experimental-json-modules --require dotenv/config ./dist/app.js\"",
        "start": "node --experimental-json-modules --require dotenv/config ./dist/app.js",
        "test": "npm run watch"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/floorbot/floorbot.git"
    },
    "keywords": [
        "floorbot"
    ],
    "author": "Arcadeena",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/floorbot/floorbot/issues"
    },
    "homepage": "https://github.com/floorbot/floorbot#readme",
    "devDependencies": {
        "tsc-watch": "^4.5.0",
        "typescript": "^4.4.4"
    },
    "dependencies": {
        "@types/async-exit-hook": "^2.0.0",
        "@types/humanize-duration": "^3.27.1",
        "async-exit-hook": "^2.0.1",
        "discord.js": "^14.1.2",
        "discord.js-handlers": "github:floorbot/discord.js-handlers",
        "dotenv": "^16.0.1",
        "envalid": "^7.3.1",
        "humanize-duration": "^3.27.2"
    }
}
