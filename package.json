    "dependencies": {
        "@types/async-exit-hook": "^2.0.0",
        "@types/humanize-duration": "^3.27.0",
        "@types/luxon": "^2.0.5",
        "@types/nconf": "^0.10.0",
        "@types/node-fetch": "^2.5.10",
        "@types/node-schedule": "^1.3.2",
        "@types/probe-image-size": "^7.0.1",
        "@types/twemoji": "^12.1.2",
